{
  "@context": {
    "@vocab": "http://schema.org/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "schema": "http://schema.org/",
    "author": "schema:author",
    "creator": "schema:creator",
    "license": "schema:license",
    "contactPoint": "schema:contactPoint",
    "description": "schema:description",
    "identifier": "schema:identifier",
    "keywords": "schema:keywords",
    "name": "schema:name",
    "spatialCoverage": "schema:spatialCoverage",
    "variableMeasured": "schema:variableMeasured",
    "funding": "schema:funding",
    "distribution": "dcat:distribution",
    "format": "dcat:mediaType"
  },
  "@type": "Dataset",
  "name": "[Insert Dataset Title]",
  "description": "[Provide a brief description of the dataset. What kind of data is included?]",
  "author": {
    "@type": "Person",
    "name": "[Insert Principal Investigator's Name or Organization Name]",
    "affiliation": "[Insert Institution Name]",
    "email": "[Insert Contact Email]"
  },
  "creator": {
    "@type": "Organization",
    "name": "[Insert Organization Name]"
  },
  "license": "[Insert License Type/Link (e.g., https://creativecommons.org/licenses/by/4.0/)]",
  "keywords": [
    "[Insert Keyword 1]",
    "[Insert Keyword 2]",
    "[Insert Keyword 3]",
    "[Insert more relevant keywords]"
  ],
  "identifier": {
    "@type": "PropertyValue",
    "name": "[Insert Identifier Type (e.g., Dataset DOI, File ID)]",
    "value": "[Insert Identifier Value]"
  },
  "dateCreated": "[Insert Date of Data Collection]",
  "spatialCoverage": {
    "@type": "Place",
    "name": "[Insert Geographic Location or Scope]"
  },
  "funding": {
    "@type": "MonetaryGrant",
    "name": "[Insert Funding Source (if applicable)]"
  },
  "distribution": [
    {
      "@type": "DataDownload",
      "name": "[Insert File Name (e.g., datafile.csv, dataset.nc)]",
      "encodingFormat": "[Insert File Format (e.g., application/csv, application/x-netcdf)]",
      "contentUrl": "[Insert Link to Dataset or File Download URL]"
    }
  ],
  "variableMeasured": [
    {
      "@type": "PropertyValue",
      "name": "[Insert Variable 1 Name (e.g., Temperature)]",
      "unitText": "[Insert Unit of Measurement (e.g., °C)]"
    },
    {
      "@type": "PropertyValue",
      "name": "[Insert Variable 2 Name]",
      "unitText": "[Insert Unit of Measurement]"
    }
    // Add more variables as needed
  ],
  "contactPoint": {
    "@type": "ContactPoint",
    "name": "[Insert Contact Name]",
    "email": "[Insert Contact Email]",
    "contactType": "Principal Investigator or Dataset Manager"
  },
  "relatedLink": [
    {
      "@type": "ScholarlyArticle",
      "name": "[Insert Related Publication Title (if applicable)]",
      "url": "[Insert Link to Related Article or Dataset]"
    }
  ],
  "methodology": {
    "@type": "CreativeWork",
    "name": "Methodological Information",
    "description": "[Provide a description of the methods used to collect/generate the data. Include any relevant software, instruments, or experimental protocols.]",
    "softwareRequirements": {
      "@type": "SoftwareApplication",
      "name": "[Insert Software Used (if applicable)]",
      "softwareVersion": "[Insert Software Version (if applicable)]"
    }
  },
  "version": "[Insert Dataset Version]",
  "variableInfo": {
    "@type": "Dataset",
    "name": "Variables",
    "description": "This dataset contains the following variables:",
    "variableMeasured": [
      {
        "@type": "PropertyValue",
        "name": "[Insert Variable Name]",
        "description": "[Describe the variable (e.g., Water temperature)]",
        "unitText": "[Insert Unit (e.g., °C)]"
      },
      {
        "@type": "PropertyValue",
        "name": "[Insert Variable Name]",
        "description": "[Describe the variable]",
        "unitText": "[Insert Unit]"
      }
      // Repeat for other variables
    ]
  }
}